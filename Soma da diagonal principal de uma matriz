#include <stdio.h>

// Função principal
int main() {
    int matriz[3][3] = {
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
    };
    int soma = 0;

    // Soma dos elementos da diagonal principal
    for (int i = 0; i < 3; i++) {
        soma += matriz[i][i];
    }

