#include <stdio.h>

// Função para calcular a média
float calcularMedia(int vetor[], int tamanho) {
    int soma = 0;
    for (int i = 0; i < tamanho; i++) {
        soma += vetor[i];
    }
    return (float)soma / tamanho;
}

// Função principal
int main() {
    int vetor[6] = {10, 20, 30, 40, 50, 60};
    
    float media = calcularMedia(vetor, 6); // Chamada da função
    printf("A média dos elementos do vetor é: %.2f\n", media);

    return 0;
}
