#include <stdio.h>

// Função principal
int main() {
    int n, fatorial = 1;

    printf("Digite um número para calcular o fatorial: ");
    scanf("%d", &n);

    // Calcula o fatorial
    for (int i = 1; i <= n; i++) {
        fatorial *= i;
    }

    printf("O fatorial de %d é: %d\n", n, fatorial);
    return 0;
}
