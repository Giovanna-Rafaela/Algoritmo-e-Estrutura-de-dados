#include <stdio.h>

// Função principal
int main() {
    int tamanho = 8;

    for (int i = 0; i < tamanho; i++) {
        for (int j = 0; j < tamanho; j++) {
            if ((i + j) % 2 == 0) {
                printf("■ ");
            } else {
                printf("□ ");
            }
        }
        printf("\n");
    }

    return 0;
}
