#include <stdio.h>

// Função principal
int main() {
    int vetor[10] = {3, 6, 9, 12, 15, 18, 21, 24, 27, 30};
    int pares = 0, impares = 0;

    // Conta os números pares e ímpares
    for (int i = 0; i < 10; i++) {
        if (vetor[i] % 2 == 0) {
            pares++;
        } else {
            impares++;
        }
    }

    printf("Números pares: %d\n", pares);
    printf("Números ímpares: %d\n", impares);

    return 0;
}
